<?xml version="1.0" encoding="UTF-8" ?>

<configuration>

	<appender name="console" class="ch.qos.logback.core.ConsoleAppender">
		<layout>
			<pattern>
				%d{HH:mm:ss.SSS} [%thread] %-5level %logger{32} - %msg%n
			</pattern>
		</layout>
	</appender>

	<root level="DEBUG">
		<appender-ref ref="console" />
	</root>

</configuration>
